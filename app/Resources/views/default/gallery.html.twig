{% extends 'base.html.twig' %}


{% block body %}
<div class='container'>
	<div class='row'>
		{% for image in images %}
		<div class="col-xs-4 col-sm-3 col-md-3 col-lg-3">
			<a href="{{ "image/#{image.id}" }}" class="thumbnail"> <img class="img-responsive"
				src="{{ vich_uploader_asset(image, 'image', 'AppBundle\\Entity\\Image') }}"
				style="height:200px;">
			</a>
		</div>
		{% endfor %}
	</div>
	<!-- end row -->
</div>
{% endblock %}