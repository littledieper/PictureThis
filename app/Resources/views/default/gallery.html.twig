{% extends 'base.html.twig' %}

{% block body %}
<div class='container'>
	{% for image in images %}
		<div class="col-xs-4 col-sm-3 col-md-3 col-lg-3">
        		<a href="{{ "image/#{image.id}" }}">
          			<img class="img-thumbnail"
          			src="{{ vich_uploader_asset(image, 'image', 'AppBundle\\Entity\\Image') }}" height = 256px>
        		</a>
        	</div>
	{% endfor %}
</div>
{% endblock %}